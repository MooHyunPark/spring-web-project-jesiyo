<style>
    @font-face {
        font-family: 'contentFont'; /* 폰트 이름 지정 */
        src: url('/fonts/BMDOHYEON.ttf') format('truetype'); /* 경로와 파일 형식 */
    }

    .page-item {
        font-family: 'contentFont', serif;
    }
</style>

<nav aria-label="Page navigation example">
    <ul class="pagination justify-content-center">
        {{#pagination.pages}}
            <li class="page-item {{#isActive}}active{{/isActive}}">
                <a class="page-link"
                   style="background-color: #f96506; border-color: #f96506; color: {{#isActive}}#FFFF00{{/isActive}}{{^isActive}}#CCCCCC{{/isActive}};"
                   href="#"
                   onclick="changePage({{pageNumber}})">{{pageNumber}}</a>
            </li>
        {{/pagination.pages}}
    </ul>
</nav>
